{
  "name": "cabinet",
  "version": "1.0.0",
  "description": "Projet M2 MIASHS de cabinet m√©dical",
  "main": "server.js",
  "scripts": {
    "start": "node ./appJS/server.js",
    "compile": "rm -Rf ./appJS && node ./node_modules/typescript/bin/tsc",
    "compileWatch": "rm -Rf ./appJS && node ./node_modules/typescript/bin/tsc --watch",
    "lint": "node ./node_modules/tslint/bin/tslint --config \"./tslint.yaml\" \"app/**/*.ts\"",
    "watch:lint": "node ./node_modules/chokidar-cli/index.js 'app/**/*.ts' -c \"npm run lint\" --initial --verbose",
    "compileAndLint": "npm-run-all --parallel watch:lint compileWatch",
    "tscwatch": "rm -Rf ./appJS/ && node ./node_modules/tsc-watch --onSuccess 'yarn lint && node ./appJS/server.js'",
    "tscwatchNoLintBareMongo": "rm -Rf ./appJS/ && node ./node_modules/tsc-watch --onSuccess 'node ./appJS/server.js db=baremongo'",
    "tscwatchNoLintMongoose": "rm -Rf ./appJS/ && node ./node_modules/tsc-watch --onSuccess 'node ./appJS/server.js db=mongoose'"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.18.2",
    "bson": "^1.0.4",
    "console-error": "^0.0.4",
    "console-info": "^0.0.5",
    "console-warn": "^0.0.4",
    "express": "^4.16.2",
    "fs-extra": "^4.0.1",
    "mongodb": "2.2",
    "mongoose": "^5.0.1",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "@types/express": "^4.11.0",
    "@types/fs-extra": "^5.0.0",
    "@types/mongodb": "2.2",
    "@types/mongoose": "^5.0.0",
    "@types/node": "^8.0.26",
    "chokidar-cli": "^1.2.0",
    "module-alias": "^2.0.1",
    "npm-run-all": "^4.1.1",
    "tsc-watch": "^1.0.13",
    "tslint": "^5.9.1",
    "typescript": "^2.6.2"
  }
}
